    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
  "dependencies": {
    "babel-preset-vue-app": "^1.2.0",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "caniuse-lite": "^1.0.30000696",
    "cookie-session": "^2.0.0-beta.2",
    "express": "^4.15.4",
    "jquery": "^3.2.1",
    "request": "^2.81.0",
    "semantic-ui": "^2.2.13",
    "style-loader": "^0.18.2",
    "vue": "^2.3.3",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1"
  },
  "devDependencies": {},
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
